   ],
   "source": [
    "l=[\"Mahesh\",\"Raj\",\"Soha\",\"Himanshu\",\"Rohan\"]\n",
    "x=len(l[0])\n",
    "for i in l:\n",
    "    z=len(i)\n",
    "    if z<x:\n",
    "        y=i\n",
    "        x=z\n",
    "y"
   ]
