   "source": [
    "# Check user input number is palindrome or not."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "72be7f2a",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter Any No.=424\n",
      "424\n",
      "Palindrome No.:\n"
     ]
    }
   ],
   "source": [
    "n=int(input(\"Enter Any No.=\"))\n",
    "x=n\n",
    "rev=0\n",
    "while n!=0:\n",
    "    d=n%10\n",
    "    rev=rev*10+d\n",
    "    n=n//10\n",
    "\n",
    "print(rev)\n",
    "if rev==x:\n",
    "    print(\"Palindrome No.:\")\n",
    "else:\n",
    "    print(\"Not a Palindrome No.:\")"
   ]
  }
 ],
